{
  "name": "cowlick-kag-compiler",
  "version": "0.1.0",
  "description": "cowlick kag compiler",
  "main": "lib/index.js",
  "bin": {
    "cowlick-kag-compiler": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "resources"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p ./src && npm run pegjs",
    "test": "mocha --compilers ts:espower-typescript/guess test/*.ts",
    "pegjs": "pegjs --output ./resources/kag.js ./resources/kag.pegjs"
  },
  "author": "pocketberserker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowlick/cowlick.git"
  },
  "dependencies": {
    "acorn": "5.2.1",
    "commandpost": "1.2.1",
    "cowlick-analyzer": "^0.1.0",
    "cowlick-core": "^0.1.0",
    "escodegen": "1.9.0",
    "estraverse": "4.2.0",
    "ora": "1.3.0"
  },
  "devDependencies": {
    "@akashic/akashic-engine": "2.0.2",
    "@types/acorn": "4.0.3",
    "@types/escodegen": "0.0.6",
    "@types/esprima": "4.0.1",
    "@types/estraverse": "0.0.6",
    "@types/glob": "5.0.33",
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.53",
    "@types/ora": "1.3.1",
    "@xnv/headless-akashic": "0.0.1",
    "espower-typescript": "8.1.2",
    "esprima": "4.0.0",
    "mocha": "3.5.3",
    "pegjs": "0.10.0",
    "power-assert": "1.4.4",
    "typescript": "2.6.2"
  }
}
