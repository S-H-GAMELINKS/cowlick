{
  "name": "cowlick-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cowlick-kag-compiler ./scenario -o ./src && webpack --progress && npm run scan:assets",
    "start": "akashic-sandbox game",
    "scan:assets": "cd ./game && akashic scan asset",
    "bundle": "cd ./game && akashic export html --output ../dist && cp ../../../docs/images/favicon.ico ../dist"
  },
  "dependencies": {
    "cowlick-engine": "^0.1.0"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "1.2.1",
    "@akashic/akashic-engine": "2.0.0",
    "@akashic/akashic-sandbox": "0.13.4",
    "@types/node": "8.0.53",
    "copy-webpack-plugin": "4.2.1",
    "cowlick-kag-compiler": "^0.1.0",
    "ts-loader": "3.1.1",
    "typescript": "2.6.1",
    "uglify-save-license": "0.4.1",
    "uglifyjs-webpack-plugin": "1.1.0",
    "webpack": "3.8.1"
  }
}
