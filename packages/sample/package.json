{
  "name": "cowlick-sample",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "cowlick-kag-compiler ./scenario -o ./script && tsc -p . && npm run scan:assets",
    "start": "akashic-sandbox .",
    "scan:assets": "npm run copy:images && akashic scan asset",
    "copy:images": "cp ../../docs/images/cowlick.png ./image",
    "bundle": "akashic export html --output ../../dist -M --strip && cp ../../docs/images/favicon.ico ../../dist"
  },
  "dependencies": {
    "@akashic-extension/akashic-label": "2.0.2",
    "@akashic-extension/akashic-timeline": "2.0.0",
    "@pocketberserker/akashic-pagination": "0.0.4",
    "@xnv/akashic-scrollable": "0.0.4",
    "cowlick-config": "^0.3.0",
    "cowlick-core": "^0.3.0",
    "cowlick-engine": "^0.3.0"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "1.3.0",
    "@akashic/akashic-engine": "2.0.3",
    "@akashic/akashic-sandbox": "0.13.12",
    "@types/node": "8.5.2",
    "copy-webpack-plugin": "4.3.1",
    "cowlick-kag-compiler": "^0.3.0",
    "typescript": "2.6.2"
  }
}
