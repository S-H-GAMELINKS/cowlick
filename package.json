{
  "name": "cowlick",
  "version": "0.0.1",
  "description": "novel engine for Akashic Engine",
  "main": "index.js",
  "bin": {
    "cowlick": "./bin/cowlick"
  },
  "files": [
    "bin",
    "lib",
    "resources",
    "index.js"
  ],
  "scripts": {
    "postinstall": "cd sample && npm install",
    "build": "tsc -p . && npm run pegjs",
    "sample:build": "cd sample && npm run build",
    "sample:bundle": "cd sample && npm run bundle",
    "sample:publish": "gh-pages -d ./sample/dist -m \"Update $(date +%Y-%m-%d:%H:%M:%s)\"",
    "sample:deploy": "npm run sample:bundle && npm run sample:publish",
    "start": "cd sample && npm start",
    "lint": "eslint --fix --ext .ts src",
    "test": "mocha --compilers ts:espower-typescript/guess test/*.ts",
    "pegjs": "pegjs --output ./resources/kag.js ./resources/kag.pegjs"
  },
  "author": "pocketberserker",
  "license": "MIT",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "pocketberserker/cowlick.git"
  },
  "dependencies": {
    "@akashic-extension/akashic-label": "0.3.4",
    "@akashic-extension/akashic-timeline": "0.4.2",
    "@xnv/akashic-scrollable": "0.0.1",
    "commandpost": "1.2.1",
    "escodegen": "1.9.0",
    "esprima": "4.0.0",
    "estraverse": "4.2.0"
  },
  "devDependencies": {
    "@akashic/akashic-engine": "2.0.0",
    "@types/escodegen": "0.0.6",
    "@types/esprima": "4.0.1",
    "@types/estraverse": "0.0.6",
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.28",
    "eslint": "4.7.1",
    "espower-typescript": "8.1.1",
    "gh-pages": "1.0.0",
    "mocha": "3.5.3",
    "pegjs": "0.10.0",
    "power-assert": "1.4.4",
    "typescript": "2.5.3",
    "typescript-eslint-parser": "8.0.0"
  }
}
